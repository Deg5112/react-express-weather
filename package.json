{
  "name": "express-react-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@material-ui/core": "^4.11.3",
    "axios": "^0.21.1",
    "body-parser": "^1.17.2",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.3",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.15.3",
    "http-proxy-middleware": "^1.0.6",
    "jade": "^1.11.0",
    "moment": "^2.29.1",
    "morgan": "^1.8.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.2",
    "react-test-renderer": "^17.0.1",
    "react-toast": "^1.0.1",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7"
  },
  "scripts": {
    "start": "node -r esm server/server.js",
    "cracoStart": "craco -r esm start",
    "startWithNodeBackend": "concurrently \"craco start\" \"nodemon -r esm server/server.js\"",
    "build": "craco build && mv build server",
    "test": "craco test --env=jsdom",
    "eject": "react-scripts eject",
    "debug": "nodemon server/server.js",
    "kill": "kill $(lsof -t -i:3001) | exit 0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "pages/(.*)": "<rootDir>/src/pages/$1",
      "images/(.*)": "<rootDir>/src/images/$1",
      "ServiceProvider/(.*)": "<rootDir>/src/services/substituted_module_$1.js"
    }
  }
}
